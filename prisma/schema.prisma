generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  directUrl = env("DIRECT_URL" )
}

model users {
  id Int @id @default(autoincrement())
  auth_id String? @unique
  fullName String @unique
  email String
  password String 
}

model product {
  id Int @id @default(autoincrement())
  namaProduk String
  harga Int
  deskripsi String?
  gambar String?
  linkShoppe String?
  linkTokopedia String?
  user_id String?
  timeAdded DateTime
}

model tanaman {
  id Int @id @default(autoincrement())
  namaTanaman String
  deskripsi String?
  gambar String?
  user_id String?
  timeAdded DateTime
}

